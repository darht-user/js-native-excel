{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"object-curly-spacing": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"eol-last": "off",
		"operator-linebreak": "off",
		"no-trailing-spaces": "off",
		"max-len": "off",
		"no-unused-vars": "warn",
		"indent": "off",
		"quotes": "warn"
	},
	"extends": ["eslint:recommended", "google"]
} 